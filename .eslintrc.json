{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": "off",
    "func-names": "off",
    "react/function-component-definition": "off",
    "jsx-quotes": "off",
    "import/no-named-as-default-member": "off",
    "import/no-named-as-default": "off",
    "import/named": "off",
    "no-unused-vars": "warn",
    "import/prefer-default-export": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off"
  }
}
